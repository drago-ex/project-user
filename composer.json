{
	"name": "drago-ex/project-user",
	"type": "composer-plugin",
	"require": {
		"php": ">=8.3 <9",
		"drago-ex/database": "^2.0",
		"nette/security": "^3.2",
		"nette/di": "^3.2",
		"nette/application": "^3.2",
		"composer-plugin-api": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"Drago\\": "src/"
		}
	},
	"extra": {
		"class": "Drago\\Composer\\Plugin"
	},
	"require-dev": {
		"composer/composer": "^2.9",
		"phpstan/phpstan-nette": "^1.2.9"
	},
	"scripts": {
		"phpstan": "phpstan analyse --level 5 --configuration phpstan.neon User"
	}
}
